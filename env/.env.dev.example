# Set DB_HOST=warehouse-db when using the Docker Postgres service
DB_HOST=localhost
DB_NAME=survivor_dw_dev
DB_USER=survivor_dev
DB_PASSWORD=survivor_dev_password
PORT=5432
SURVIVOR_ENV=dev
# Control pipeline depth (bronze, silver, gold)
GAMEBOT_TARGET_LAYER=gold
# Airflow DAG schedule (cron)
GAMEBOT_DAG_SCHEDULE=0 4 * * 1
# Airflow webserver port (defaults to 8080)
AIRFLOW_PORT=8080
# Airflow API rate limiting storage (shared Redis backend)
AIRFLOW__API_RATELIMIT__STORAGE=redis://redis:6379/1
AIRFLOW__API_RATELIMIT__ENABLED=True
